<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/mainActivity">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.weterings.ikpmd.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" >
        <action
            android:id="@+id/action_mainActivity_to_collection2"
            app:destination="@id/collection"
            app:enterAnim="@anim/nav_default_enter_anim" />
    </activity>
    <activity
        android:id="@+id/collection"
        android:name="com.weterings.ikpmd.Collection"
        android:label="activity_collection"
        tools:layout="@layout/activity_collection" />
    <activity
        android:id="@+id/summonActivity"
        android:name="com.weterings.ikpmd.SummonActivity"
        android:label="SummonActivity" />
</navigation>