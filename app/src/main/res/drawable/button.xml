<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#33BEFF" />
            <gradient android:angle="-90" android:startColor="#33BEFF" android:endColor="#33BEFF"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#33BEFF" />
            <solid android:color="#33BEFF"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#33BEFF" />
            <gradient android:angle="-90" android:startColor="#33BEFF" android:endColor="#1D95CD" />
        </shape>
    </item>
</selector>